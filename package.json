{
  "name": "@jimengio/code-rule-editor",
  "version": "0.1.1",
  "description": "Code rule editor of JimengIO",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/code-rule-editor",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "JIMENGIO",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/doc-frame": "^0.1.11",
    "@jimengio/flex-styles": "^0.1.6",
    "@jimengio/jimo-basics": "^0.0.13-a1",
    "@jimengio/jimo-icons": "^0.3.4",
    "@jimengio/meson-modal": "^0.3.3",
    "@jimengio/router-code-generator": "^0.2.6-a1",
    "@jimengio/ruled-router": "^0.2.24-a5",
    "@jimengio/safe-property": "^0.0.2",
    "@jimengio/shared-utils": "^0.1.5",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^13.7.1",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "antd": "^3.26.11",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.4.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.12.1",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "dayjs": "^1.8.20",
    "decimal.js": "^10.2.0",
    "emotion": "^10.0.27",
    "immer": "^5.3.6",
    "lodash-es": "^4.17.15",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "peerDependencies": {
    "@jimengio/flex-styles": ">=0.1.6",
    "@jimengio/jimo-basics": "^0.0.8",
    "@jimengio/jimo-icons": "^0.3.2",
    "@jimengio/meson-modal": "*",
    "@jimengio/safe-property": "*",
    "@jimengio/shared-utils": "*"
  }
