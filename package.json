{
  "name": "@jimengio/code-rule-editor",
  "version": "0.2.0",
  "description": "Code rule editor of JimengIO",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "rm -rfv lib/* && tsc -d --project tsconfig-compile.json --outDir lib/",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/code-rule-editor",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "JIMENGIO",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/doc-frame": "^0.1.13",
    "@jimengio/flex-styles": "^0.2.0",
    "@jimengio/jimo-basics": "^0.1.11",
    "@jimengio/jimo-icons": "^0.3.14",
    "@jimengio/meson-modal": "^0.3.10",
    "@jimengio/router-code-generator": "^0.3.0",
    "@jimengio/ruled-router": "^0.2.27",
    "@jimengio/safe-property": "^0.0.2",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.6.4",
    "@types/prettier": "^2.1.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.5",
    "antd": "^4.6.3",
    "cache-loader": "^4.1.0",
    "css-loader": "^4.2.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "html-webpack-plugin": "^4.4.1",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.12.1",
    "mini-css-extract-plugin": "^0.11.0",
    "prettier": "^2.1.1",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.2",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "dayjs": "^1.8.35",
    "decimal.js": "^10.2.0",
    "emotion": "^10.0.27",
    "immer": "^9.0.6",
    "lodash-es": "^4.17.15",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "peerDependencies": {
    "@jimengio/flex-styles": ">=0.1.6",
    "@jimengio/jimo-basics": "^0.0.8",
    "@jimengio/jimo-icons": "^0.3.2",
    "@jimengio/meson-modal": "*",
    "@jimengio/safe-property": "*"
  }
}
