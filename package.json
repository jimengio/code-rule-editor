{
  "name": "@jimengio/code-rule-editor",
  "version": "0.1.0-a3",
  "description": "Code rule editor of JimengIO",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "prepare": "yarn compile",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/code-rule-editor",
    "gen-router": "ts-node -P tsconfig-node.json -r tsconfig-paths/register example/gen-router.ts",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "JIMENGIO",
  "license": "MIT",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/doc-frame": "^0.1.2-a4",
    "@jimengio/flex-styles": "^0.1.6",
    "@jimengio/meson-modal": "^0.2.14-a1",
    "@jimengio/router-code-generator": "^0.2.0",
    "@jimengio/ruled-router": "^0.2.15",
    "@jimengio/safe-property": "^0.0.2",
    "@jimengio/shared-utils": "^0.1.5",
    "@types/antd": "^1.0.0",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.12",
    "@types/prettier": "^1.18.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "antd": "^3.23.6",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.11.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.4",
    "typescript-styled-plugin": "^0.14.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "dayjs": "^1.8.16",
    "decimal.js": "^10.2.0",
    "emotion": "^10.0.17",
    "immer": "^4.0.1",
    "lodash": "^4.17.15",
    "query-string": "^6.8.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "peerDependencies": {
    "@jimengio/flex-styles": ">=0.1.6",
    "@jimengio/meson-modal": "*",
    "@jimengio/safe-property": "*",
    "@jimengio/shared-utils": "*"
  }
}
